<%= superadmin_tabs_generator() %>
<div class="objectListWithTab">
	<div class="blockCornerLeft"></div><div class="blockElementHeader sevH">
		<span id="item_count">Google Analytics Settings</span>
	</div><div class="blockCornerRight"></div>
	<div class="contentList superadmin">
	<% form_for :analytic_config, @analytic_config, :url => updating_superadmin_google_analytics_path,
  :html => {:method => :put, :multipart => true, :class => "fieldsetSimulation"} do |f| -%>
      <label>Login:</label>
      <div class="formElement">
        <%= f.text_field :sa_analytic_login %>
      </div>
      <label>Password:</label>
      <div class="formElement">
        <%= f.password_field :sa_analytic_password %>
      </div>
		 <button type="submit" class="submitButton"><span><%=I18n.t('general.button.update')%></span></button>
		<% end %>
	</div>
</div>
