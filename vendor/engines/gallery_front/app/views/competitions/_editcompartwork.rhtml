
<style>
  .changecolorgray{
    color: #444444;
    line-height: 15px;
  }
</style>
<div  class="inputfont">
    <div id="messageforimageuploaded">
    <%if !messageforimageuploaded.blank?%>
        <%=messageforimageuploaded%>
    <%end%>
    </div>

    
 <form action="edit_upload_the_artwork_to_competition" method="post" enctype="multipart/form-data"  onsubmit="return  Checkfiles_simple('<%=updateimagearray[7]%>',true)" id="add_the_artwork_to_competition<%=updateimagearray[7]%>"  target="upload_frame<%=updateimagearray[7]%>">  
<div class="changecolorgray">   
  Title 
</div>  
  <input  name="competitions_user[worktitle]" id="artwork_title<%=updateimagearray[7]%>" style="width:310px;height: 14px;" value="<%=updateimagearray[0]%>" type="text"  >
<div class="changecolorgray">     
  Medium
</div>  
  <input  name="competitions_user[workmedium]" id="artwork_medium<%=updateimagearray[7]%>"  value="<%=updateimagearray[1]%>" type="text" style="width:310px;height: 14px;">
<div class="changecolorgray">     
<span style="font-size: 10px;">Size (width (cm) x height (cm)  x depth (cm - 3d work only)</span>
</div>	
<font style="color: #444444">

	<input  name="competitions_user[worksize1]" id="artwork_width<%=updateimagearray[7]%>"  value="<%=updateimagearray[2].split('x')[0]%>" type="text" style="width: 92px;height: 14px;">
          x
  <input  name="competitions_user[worksize2]" id="artwork_height<%=updateimagearray[7]%>"  value="<%=updateimagearray[2].split('x')[1]%>" type="text" style="width: 92px;height: 14px;">
          x
   <input  name="competitions_user[worksize3]" id="artwork_depth<%=updateimagearray[7]%>"  value="<%=updateimagearray[2].split('x')[2]%>" type="text" style="width: 92px;height: 14px;">
</font>
<div class="changecolorgray">      
    <span style="padding-right:90px">
                               Price
                           </span> 
                          <span style="padding-right:50px">
                               Edition From
                          </span> 
                        <span>
                               Edition Number
                          </span> 
</div>  
   <input  name="competitions_user[workprice]" id="artwork_price<%=updateimagearray[7]%>" size="40" value="<%=updateimagearray[4]%>" type="text" style="width: 92px;height: 14px;">
   <input  name="competitions_user[editionname]" id="artwork_edition_name<%=updateimagearray[7]%>" size="40" value="<%=updateimagearray[5]%>" type="text" style="width: 92px;height: 14px;margin-left:10px;">
   <span style="padding-right:3px;margin-left:3px;color:#444444">of</span><input  name="competitions_user[editionnumber]" id="artwork_edition_number<%=updateimagearray[7]%>" size="40" value="<%=updateimagearray[6]%>" type="text" style="width: 82px;height: 14px;">

   
<div class="changecolorgray">         
	 Image
</div>
	 <input  name="competitions_user[workimage]" id="artwork_image<%=updateimagearray[7]%>" type="file" style="height:28px;float:left"/>
	 <%=hidden_field_tag "competitionuserid",competitionuser%>
	 <%=hidden_field_tag "titleforupdate",updateimagearray[7] %>
   
          <div style="float:left">
                   <%= submit_tag 'Upload'%>
          </div>  
          <div style="display: none;padding-top:2px;" id="show_ajax_request<%=updateimagearray[7]%>">
                     <img height="20" src="/images/ajax-spinner.gif" alt="loading">
          </div>

        <div style="clear:both" id="show_ajax_request<%=updateimagearray[7]%>">
          <img src="/system/gallery/<%=updateimagearray[3]%>" width="90px" height="90px"/>   
        </div>  
         
</div>
</form>
<iframe id="'upload_frame<%=updateimagearray[7]%>"   name="upload_frame<%=updateimagearray[7]%>" style="width:1px;height:1px;display:none"></iframe>

